    <!-- Side bar customzation -->
<style>
  table,tr,td,th,tbody{
    text-align:center;
  }
  table#tipo_table
  {
    width:280px; 
  }
  
  table tr,td
  {
    border: none !important;
  }
  
  img
  {
    width:100px; 
    cursor:pointer;
  }
  
  .tipo-img
  {
    width: 90px !important; 
  }
  
  .intensidad-img
  {
    width: 90px !important; 
  }
  
  .intensidad-img:hover
  {
    width: 90px !important; 
    border:3px solid #2196F3;
  }
  
  .tipo-img.img-checked, .origen-img.img-checked, .certificados-img.img-checked, .tostado-img.img-checked, .proceso-img.img-checked
  {
    border: 3px solid #2cd63c;
    box-shadow: 5px 10px 18px #888888;
  }
  
  .intensidad-img.img-checked
  {
    border: 3px solid #2cd63c;
    box-shadow: 5px 10px 18px #888888;
  }
  
  .notas-img.img-checked
  {
    border: 3px solid #2cd63c;
    box-shadow: 5px 10px 18px #888888;
  }
  
  
  
  td img:hover
  {
    border:2px solid #2196F3;
    
  }
  .img-checked
  {
    border:3px solid #646464;
  }
  .tr-divider{
     height: 30px; 
  }
  .table-div
  {
    overflow-x: auto; 
  }
  .scroll-top-margin
  {
    margin-top:20px;  
  }
  label
  {
    cursor:pointer; 
  }
  
  .th-title
  {
     background-color: #262626;color: white;text-align: center;font-size: 20px;height: 35px;width:40%; border-bottom: 1px solid #838383;
  }
  .th-margin
  {
  	border-left: none;border-right: none;border-top: none;border-bottom: 1px solid #838383;background: white;width:60%;
  }
  
  .clear-filter
  {
    text-align:center;
    margin-bottom:30px;
  }
  
  .clear-filter-title
  {
    color: #373737 !important;
    font-size: 1rem; 
  }
  
  .clear-filter-title:hover
  {
    color: #B2A18A !important;
  }
  
  .breadcrumb-collection
  {
 	display:none;
  }
  .toggle-all--true{
    display:none;
  }
  
  .carousel-cell{
     width:300px; 
  }
  
  .clear-filter-title{
     cursor: pointer;
  }
  .flickity-prev-next-button
  {
    background: transparent !important; 
  }
  .sidebar-container
  {
    margin-right: 45px;
    margin-left: -25px;
  }
  
  .mobile-show
  {
    display:none !important;
  }
  .mobile-hide
  {
     display:initial !important;
  }
  
  @media (max-width: 480px)
  { 
    .mobile-show
    {
      display:initial !important;
    }
    .mobile-hide
    {
      display:none !important;
    }
    td img:hover {
      border: none;
    }
    .sidebar-container
    {
      margin-right: 0px;
      margin-left: 0px;
    }
    .flickity-viewport
    {
      height:115px !important; 
    }
    .collection_description{
      display:block; 
    }
    
  }
  
</style>

